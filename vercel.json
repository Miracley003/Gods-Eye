{
  "version": 2,
  "name": "gods-eye",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_APP_TITLE": "Gods Eye",
    "VITE_APP_DESCRIPTION": "Cross-Chain Wallet Forensics & Security"
  }
}
